<%- include('./includes/head.ejs') %>
    <main>
        <div class="login-box">
            <form class="form">
                <div class="form-group">
                  <label for="username" class="">Username</label>
                  <input type="text" class="form-control" id="username" placeholder="abc123">
                </div>

                <div class="form-group">
                    <label for="password" class="">Password</label>
                    <input type="password" class="form-control" id="password" placeholder="********">
                  </div>

                <div class="form-submit">
                    <button type="submit" class="btn sign-in">Login</button>
                    <button type="submit" class="btn sign-up">Sign Up</button>
                </div>
                
                <div class="forget-pass">
                    <a href="/auth/forget-password">Forget Password</a>
                </div>
              </form>
        </div>
    </main>
<%- include('./includes/end.ejs')%>